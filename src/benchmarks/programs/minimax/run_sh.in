#!/bin/bash

NAME="ttt"
DESCRIPTION="tic tac toe game tree generated by minimax algorithm"

## Parameters provided by the configure script

# number of times to test each data point; for statistical validity
NUM_TRIALS="@NUM_TRIALS@"
MANTI_BENCH_ROOT=@MANTI_BENCH_ROOT@
# generate different processor configurations used by the experiment
# e.g., -p 1 -p 2 ... -p num_procs
PROCS_LIST=""
for i in {1..@NUM_PROCS@}; do
    PROCS_LIST="$PROCS_LIST -p $i"
done

# parallel 
$MANTI_BENCH_ROOT/scripts/experiment.sh $PROCS_LIST -f ttt-par -n $NAME -d "$DESCRIPTION" -t $NUM_TRIALS

# sequential elison 
$MANTI_BENCH_ROOT/scripts/experiment.sh -e -f ttt-seq-elison -n $NAME -d "$DESCRIPTION" -t $NUM_TRIALS 

# sequential
$MANTI_BENCH_ROOT/scripts/experiment.sh -e -f ttt-seq -n $NAME -d "$DESCRIPTION -- sequential version" -t $NUM_TRIALS 

# smlnj
$MANTI_BENCH_ROOT/scripts/experiment-smlnj.sh -f ttt-smlnj -n "$NAME" -d "$DESCRIPTION" -t $NUM_TRIALS

# mlton
$MANTI_BENCH_ROOT/scripts/experiment-mlton.sh -f ttt-mlton -n "$NAME" -d "$DESCRIPTION" -t $NUM_TRIALS
